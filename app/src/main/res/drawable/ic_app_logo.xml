<?xml version="1.0" encoding="utf-8"?>
<!-- Trading themed logo with candlestick chart design -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="120dp"
    android:height="120dp"
    android:viewportWidth="120"
    android:viewportHeight="120">

    <!-- Background Circle with Gradient Effect -->
    <path
        android:pathData="M60,10 A50,50 0 0,1 60,110 A50,50 0 0,1 60,10 Z"
        android:fillColor="#1976D2"
        android:fillAlpha="0.15"/>

    <!-- Outer Circle -->
    <path
        android:pathData="M60,15 A45,45 0 0,1 60,105 A45,45 0 0,1 60,15 Z"
        android:strokeColor="#1976D2"
        android:strokeWidth="3"
        android:fillColor="#00000000"/>

    <!-- Candlestick Chart - Red Candle (Left) -->
    <path
        android:pathData="M35,55 L35,70"
        android:strokeColor="#F44336"
        android:strokeWidth="2"
        android:strokeLineCap="round"/>
    <path
        android:pathData="M32,58 L38,58 L38,67 L32,67 Z"
        android:fillColor="#F44336"/>

    <!-- Candlestick Chart - Green Candle (Middle) -->
    <path
        android:pathData="M50,45 L50,75"
        android:strokeColor="#4CAF50"
        android:strokeWidth="2"
        android:strokeLineCap="round"/>
    <path
        android:pathData="M47,50 L53,50 L53,70 L47,70 Z"
        android:fillColor="#4CAF50"/>

    <!-- Candlestick Chart - Green Candle (Right) -->
    <path
        android:pathData="M65,40 L65,65"
        android:strokeColor="#4CAF50"
        android:strokeWidth="2"
        android:strokeLineCap="round"/>
    <path
        android:pathData="M62,43 L68,43 L68,62 L62,62 Z"
        android:fillColor="#4CAF50"/>

    <!-- Candlestick Chart - Red Candle (Far Right) -->
    <path
        android:pathData="M80,50 L80,68"
        android:strokeColor="#F44336"
        android:strokeWidth="2"
        android:strokeLineCap="round"/>
    <path
        android:pathData="M77,53 L83,53 L83,65 L77,65 Z"
        android:fillColor="#F44336"/>

    <!-- Trend Line (Upward) -->
    <path
        android:pathData="M30,75 L45,65 L60,55 L75,45 L90,40"
        android:strokeColor="#FF9800"
        android:strokeWidth="2.5"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:fillColor="#00000000"/>

    <!-- Signal Dots on Trend Line -->
    <circle
        android:cx="45"
        android:cy="65"
        android:r="3"
        android:fillColor="#FF9800"/>
    <circle
        android:cx="60"
        android:cy="55"
        android:r="3"
        android:fillColor="#FF9800"/>
    <circle
        android:cx="75"
        android:cy="45"
        android:r="3"
        android:fillColor="#FF9800"/>

    <!-- Up Arrow (Signal indicator) -->
    <path
        android:pathData="M90,40 L86,44 L90,42 L94,44 Z"
        android:fillColor="#4CAF50"
        android:strokeColor="#4CAF50"
        android:strokeWidth="1"/>
</vector>
